# Environment Variables untuk WhatsApp Services
# Konfigurasi Umum
NODE_ENV=production

# Port Konfigurasi
API_PORT=3000
DASHBOARD_PORT=4001

# Database MySQL (Railway)
DB_HOST=
DB_USER=
DB_PASSWORD=
DB_NAME=
DB_PORT=3306

# Secret Keys
SESSION_SECRET=your_new_secret_key
JWT_SECRET=your_new_jwt_secret_key
JWT_EXPIRES_IN=7d

# WhatsApp Configuration
MAX_CONCURRENT_CLIENTS=10

# External Services
ASSISTANT_URL=

# Puppeteer Configuration
PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium
PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true
PUPPETEER_DISABLE_DEV_SHM_USAGE=true
